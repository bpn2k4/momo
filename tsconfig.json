{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@const/*": [
        "src/const/*"
      ],
      "@hooks": [
        "src/hooks/index.ts"
      ],
      "@libs": [
        "src/libs/index.ts"
      ],
      "@libs/*": [
        "src/libs/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@slices/*": [
        "src/slices/*"
      ],
    }
  }
}